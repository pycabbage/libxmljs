{
  "name": "libxmljs",
  "author": "Cabbage Lettuce",
  "contributors": [
    "Marco Rogers",
    "Jeff Smick"
  ],
  "binary": {
    "module_name": "xmljs",
    "module_path": "./build/Release/",
    "host": "https://github.com",
    "remote_path": "./pycabbage/libxmljs/releases/download/v{version}/",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  },
  "description": "libxml bindings for v8 javascript engine",
  "version": "0.19.10",
  "scripts": {
    "install": "node-gyp configure && node-pre-gyp install --fallback-to-build --loglevel http",
    "build": "node-pre-gyp build --build-from-source --fallback-to-build",
    "dist": "node-pre-gyp package",
    "test": "node --expose_gc ./node_modules/.bin/nodeunit test"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/pycabbage/libxmljs.git"
  },
  "bugs": {
    "url": "http://github.com/pycabbage/libxmljs/issues"
  },
  "main": "./index",
  "license": "MIT",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.10",
    "bindings": "^1.5.0",
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "nodeunit": "^0.11.3",
    "semver": "^7.3.8"
  }
}